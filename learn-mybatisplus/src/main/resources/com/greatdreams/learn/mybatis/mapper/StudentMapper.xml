<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.greatdreams.learn.mybatis.mapper.StudentMapper">
    <select id="selectStudentByID" parameterType="int" resultType="com.greatdreams.learn.mybatis.model.Student">
    SELECT * FROM STUDENT WHERE ID = #{id}
</select>
</mapper>